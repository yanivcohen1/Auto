			</class>
		</classes>
	</test>
	
	<test name="WhccRegulationLinkOpen">
		<classes>
			<class name="com.wh.auto.whcc.desktop.pay.TestDepositPay">
				<methods>
					<include name="regulationLinkOpen" />
				</methods>
			</class>
		</classes>
	</test>
	
</suite>