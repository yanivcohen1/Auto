<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Payments Tests" verbose="1">

	<listeners>
               <listener class-name="com.wh.auto.common.TestListener" />
   
    </listeners>
    
	<test name="WhccDepositLimit">
		<classes>
			<class name="com.wh.auto.whcc.desktop.pay.TestDepositPay">
				<methods>
					<include name="depositLimit" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="WhccDepositLimitExtended">
		<classes>
			<class name="com.wh.auto.whcc.desktop.pay.DepositLimit" />
		</classes>
	</test>

	<test name="WhccDepositLimitMobileExtended">
		<classes>
			<class name="com.wh.auto.whcc.mobile.pay.DepositLimitMobile" />
		</classes>
	</test>

	<test name="WhccDepositLimitMobile">
		<classes>
			<class name="com.wh.auto.whcc.mobile.pay.TestDepositMobilePay">
				<methods>
					<include name="depositLimit" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="WhccSelfExcludeMobile">
		<classes>
			<class name="com.wh.auto.whcc.mobile.pay.SelfExcludeMobile" />
		</classes>
	</test>
	
	<test name="WhccSelfExclude">
		<classes>
			<class name="com.wh.auto.whcc.desktop.pay.SelfExclude" />
		</classes>
	</test>
	
	<test name="WhccTimeoutMobile">
		<classes>
			<class name="com.wh.auto.whcc.mobile.pay.TimeoutMobile" />
		</classes>
	</test>
	
	<test name="WhccTimeout">
		<classes>
			<class name="com.wh.auto.whcc.desktop.pay.Timeout" />
		</classes>
	</test>
	
	<test name="WhccDepositCancel">
		<classes>
			<class name="com.wh.auto.whcc.desktop.pay.TestDepositPay">
				<methods>
					<include name="depositCancel" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="WhccClickHereOpenWin">
		<classes>
			<class name="com.wh.auto.whcc.desktop.pay.TestDepositPay">
				<methods>
					<include name="clickHereOpenWin" />
