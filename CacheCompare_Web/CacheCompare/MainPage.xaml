<UserControl x:Class="CacheCompare.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="388" d:DesignWidth="1192" xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk">

    <Grid x:Name="LayoutRoot" Background="White" Loaded="LayoutRoot_Loaded">
        <Button Content="Start" Height="23" Margin="28,22,0,0" Name="Start_B" VerticalAlignment="Top" Click="Start_B_Click" HorizontalAlignment="Left" Width="75" />
        <Button Content="Save" Height="23" HorizontalAlignment="Left" Margin="130,22,0,0" Name="Save_B" VerticalAlignment="Top" Width="75" Click="Save_B_Click" />
        <sdk:Label Height="21" Margin="967,22,12,0" Name="Status_l" VerticalAlignment="Top" />
        <sdk:Label Height="20" Margin="228,25,0,0" Name="label2" VerticalAlignment="Top" Content="Old File:" HorizontalAlignment="Left" Width="55" />
        <sdk:Label Content="New File:" Height="20" HorizontalAlignment="Left" Margin="574,25,0,0" Name="label3" VerticalAlignment="Top" Width="55" />
        <sdk:Label Height="20" HorizontalAlignment="Left" Margin="289,23,0,0" Name="File1_l" VerticalAlignment="Top" Width="279" />
        <sdk:Label Height="20" HorizontalAlignment="Left" Margin="655,23,0,0" Name="File2_l" VerticalAlignment="Top" Width="306" />
        <sdk:DataGrid AutoGenerateColumns="True" Margin="0,51,0,0" Name="ResultGrid" />
        <Button Content="Button" Height="20" HorizontalAlignment="Left" Margin="110,25,0,0" Name="button1" VerticalAlignment="Top" Width="14" Click="button1_Click" Visibility="Collapsed" />
    </Grid>
</UserControl>